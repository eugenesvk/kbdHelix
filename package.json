{
  "name": "kbdHelix",
  "type": "module",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "d": "cross-env     NODE_ENV=development rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev": "cross-env   NODE_ENV=development rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "b": "cross-env     NODE_ENV=production  rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build": "cross-env NODE_ENV=production  rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "font-shake": "yarn font-shake1 && yarn font-shake2",
    "font-shake1": "cross-env ENVIRONMENT_IS_NODE=true node --trace-uncaught ./script/font-woff2-shake.js ./src/font/noto-sans-symbols-symbols-400-normal.woff2   ./static/font/noto-sans-symbols-symbols-400-normal.woff2   ./src/font/font-shake-subset",
    "font-shake2": "cross-env ENVIRONMENT_IS_NODE=true node --trace-uncaught ./script/font-woff2-shake.js ./src/font/noto-sans-symbols-2-symbols-400-normal.woff2 ./static/font/noto-sans-symbols-2-symbols-400-normal.woff2 ./src/font/font-shake-subset",
    "serve": "zola serve",
    "s": "zola serve",
    "lan": "./zola.xsh"
  },
  "devDependencies": {
    "@csstools/postcss-oklab-function": "^2.2.3",
    "@fontsource/noto-sans-symbols": "^4.5.12",
    "@fontsource/noto-sans-symbols-2": "^4.5.11",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@swc/core": "^1.3.67",
    "@types/node": "^20.3.2",
    "bowser": "^2.11.0",
    "cross-env": "^7.0.3",
    "fonteditor-core": "^2.1.11",
    "kolorist": "^1.8.0",
    "postcss": "^8.4.24",
    "rollup": "^3.26.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc3": "^0.8.2",
    "rollup-plugin-url-resolve": "^0.2.0",
    "sass": "^1.63.6",
    "tslib": "^2.6.0",
    "typescript": "^5.0.4",
    "vite-plugin-toml": "^0.5.0"
  }
}
